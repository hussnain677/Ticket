{
  "name": "react-native-reanimated-color-picker",
  "version": "0.0.11",
  "description": "Natively animated HSV color picker for iOS & Android",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preversion": "npm run build && npm run lint",
    "postversion": "git push && git push --tags",
    "build": "patch-package && rm -rf dist && mkdir dist && tsc -p src",
    "lint": "tslint -p src",
    "watch": "npm run build -- --watch"
  },
  "keywords": [
    "react-native",
    "reanimated",
    "color-picker"
  ],
  "files": [
    "dist",
    "src",
    "*.md",
    "LICENSE"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-image-filter-kit": "*",
    "react-native-reanimated": "*"
  },
  "devDependencies": {
    "@types/react": "^17.0.6",
    "@types/react-native": "^0.64.5",
    "patch-package": "^6.4.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-filter-kit": "^0.8.0",
    "react-native-reanimated": "^2.1.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.4"
  },
  "bugs": {
    "url": "https://github.com/iyegoroff/react-native-reanimated-color-picker/issues"
  },
  "homepage": "https://github.com/iyegoroff/react-native-reanimated-color-picker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/react-native-reanimated-color-picker.git"
  }
}
